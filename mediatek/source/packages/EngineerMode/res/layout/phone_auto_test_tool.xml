		android:layout_width="fill_parent"></EditText>
		
	<TextView android:id="@+id/TextView06" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:text="@string/phone_auto_test_08"></TextView>
	<EditText android:id="@+id/EditText06" android:layout_height="wrap_content"
		android:layout_width="fill_parent"></EditText>
	
	<Button android:layout_height="wrap_content" android:text="@string/phone_auto_test_06"
		android:layout_width="fill_parent" android:id="@+id/Button01"></Button>
	<Button android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:text="@string/phone_auto_test_07" android:id="@+id/Button02"></Button>
