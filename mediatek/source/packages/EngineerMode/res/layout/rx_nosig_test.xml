<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="vertical">

	<TableLayout android:id="@+id/TableLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrPattern"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<Spinner android:id="@+id/NSRX_PatternSpinner"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></Spinner>
		</TableRow>
		
		<TableRow android:id="@+id/TableRow04" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrPocketType" 
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<Spinner android:id="@+id/NSRX_PocketTypeSpinner"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></Spinner>
		</TableRow>

		<TableRow android:id="@+id/TableRow03" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrFrequency"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<EditText android:id="@+id/NSRX_editFrequency"
				android:numeric="integer" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:singleLine="true"></EditText>
		</TableRow>
		

		<TableRow android:id="@+id/TableRow05" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrAddress"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<EditText android:id="@+id/NSRX_editTesterAddr"
				 android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:singleLine="true" android:maxLength="8"></EditText>
		</TableRow>

	</TableLayout>
	
	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="horizontal">
		<Button android:text="@string/NSRX_StartTest" 
			android:layout_marginLeft="80dip" 
			android:layout_marginTop="10dip"
			android:id="@+id/NSRX_StartTest" 
			android:layout_width="100dip" 
			  android:layout_height="wrap_content" />		
		
	</LinearLayout>
	
	<TableLayout android:id="@+id/TableLayout01"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrPackCnt" 
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/NSRX_StrPackCnt"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>

		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrPackErrRate" 
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<TextView  android:id="@+id/NSRX_StrPackErrRate"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrPackByteCnt" 
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<TextView android:id="@+id/NSRX_StrPackByteCnt"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<TextView android:text="@string/NSRX_StrBitErrRate"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
			<TextView  android:id="@+id/NSRX_StrBitErrRate"
				android:layout_marginLeft = "3pt"
				android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		</TableRow>

	</TableLayout>
	
</LinearLayout>
