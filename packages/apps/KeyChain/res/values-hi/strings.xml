<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">की चेन</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">कोई प्रमाणपत्र नहीं मिला</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">प्रमाणपत्र चुनें</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">एप्लिकेशन %s ने एक प्रमाणपत्र का अनुरोध किया है। प्रमाणपत्र के चयन द्वारा अभी एवं भविष्य में सर्वर के साथ एप्लिकेशन को इस पहचान के उपयोग करने की अनुमति प्रदान करेगा।</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">एप्लिकेशन ने अनुरोध करने वाले सर्वर की पहचान %s के रूप में की है लेकिन आपको चाहिए कि केवल उन एप्लिकेशन को पहुंच की अनुमति दें जिसपर विश्वास हो।</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">आप प्रमाणपत्र को बाह्य भंडारण में स्थित %1$s या %2$s एक्सटेंशन वाली PKCS#12 फ़ाइल को स्थापित करके कर सकते हैं।</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">"इंस्टॉल करें"</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">अनुमति दें</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">मना किया</string> 
</resources>
