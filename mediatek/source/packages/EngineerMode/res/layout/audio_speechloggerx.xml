<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content">

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical">



		<TableLayout android:id="@+id/TableLayout01"
			android:layout_width="wrap_content" android:layout_height="wrap_content">
			<TextView android:text="@string/Audio_SpeechLogger_FileText"
				android:layout_marginLeft="3pt" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:layout_marginTop="3pt"></TextView>

			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<CheckBox android:text="@string/Audio_SpeechLogger_Enable"
					android:id="@+id/Audio_SpeechLogger_Enable" android:layout_width="fill_parent"
					android:layout_height="wrap_content"></CheckBox>
			</TableRow>
			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<RadioGroup android:id="@+id/RadioGroup1"
					android:layout_marginLeft="60pt">
					<RadioButton android:text="@string/Audio_Enable_EPL"
						android:id="@+id/Audio_SpeechLogger_EPL" android:layout_width="fill_parent"
						android:layout_height="wrap_content"></RadioButton>
					<RadioButton android:text="@string/Audio_Enable_Normalvm"
						android:id="@+id/Audio_SpeechLogger_Normalvm"
						android:layout_width="fill_parent" android:layout_height="wrap_content"></RadioButton>
				</RadioGroup>
			</TableRow>
			<!--
				<View android:layout_height="1dip"
				android:layout_width="fill_parent" android:background="#FF909090">
				</View> <TextView
				android:text="@string/Audio_SpeechLogger_LocationText"
				android:layout_marginLeft="3pt" android:layout_width="wrap_content"
				android:layout_height="wrap_content"></TextView> <TableRow
				android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content"> <CheckBox
				android:text="@string/Audio_SpeechLogger_Play"
				android:id="@+id/Audio_SpeechLogger_Play"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"></CheckBox> </TableRow>
			-->

			<View android:layout_height="1dip" android:layout_width="fill_parent"
				android:background="#FF909090" android:layout_marginTop="5pt">
			</View>


			<TextView android:text="@string/Audio_VOIPLogger_Info"
				android:layout_marginLeft="3pt" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:layout_marginTop="3pt"></TextView>

			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<CheckBox android:text="@string/Audio_VOIPLogger_Enable"
					android:id="@+id/Audio_VOIPLogger_Enable" android:layout_width="fill_parent"
					android:layout_height="wrap_content"></CheckBox>
			</TableRow>

			<View android:layout_height="1dip" android:layout_width="fill_parent"
				android:background="#FF909090" android:layout_marginTop="5pt">
			</View>


			<TextView android:text="@string/Audio_CTM4WAYLogger_EnableText"
				android:id="@+id/Audio_CTM4WAYLogger_EnableText"
				android:layout_marginLeft="3pt" android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:layout_marginTop="3pt"></TextView>

			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<CheckBox android:text="@string/Audio_CTM4WAYLogger_Enable"
					android:id="@+id/Audio_CTM4WAYLogger_Enable" android:layout_width="fill_parent"
					android:layout_height="wrap_content"></CheckBox>
			</TableRow>

		</TableLayout>
		<View android:id="@+id/Audio_View1" android:layout_height="1dip"
			android:layout_width="fill_parent" android:background="#FF909090"
			android:layout_marginTop="5pt">
		</View>
		<Button android:text="@string/Audio_Dump_SpeechDbgInfo"
			android:id="@+id/Dump_Speech_DbgInfo" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_marginTop="5pt">
		</Button>
	</LinearLayout>

</ScrollView>