<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">কি চেইন</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">কোন সার্টিফিকেট পাওয়া যায়নি</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">সার্টিফিকেট নির্বাচন করুন</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">%s অ্যাপ্লিকেশন একটি সার্টিফিকেটের জন্য অনুরোধ জানিয়েছে। একটি সার্টিফিকেট নির্বাচন অ্যাপ্লিকেশনটিকে বর্তমানে ও ভবিষ্যতে সার্ভারের সাথে এই পরিচয় ব্যবহার করার সক্ষমতা প্রদান করবে।</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">অ্যাপ্লিকেশন অনুরোধকৃত সার্ভারকে %s হিসেবে চিহ্নিত করেছে কিন্তু আপনার কেবল তখনই অ্যাপ্লিকেশনটিকে সার্টিফিকেটটি লাভের অনুমতি প্রদান করা উচিত যখন অ্যাপ্লিকেশেনটির ওপর আপনার আস্থা থাকে।</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">বাহ্যিক স্টোরেজে অবস্থিত %1$s বা %2$s এক্সটেনশনসহ PKCS#12 ফাইল থেকে আপনি সার্টিফিকেট ইনস্টল করতে পারেন।</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">ইনস্টল করুন</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">অনুমতি দিন</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">প্রত্যাখান করুন</string> 
</resources>
