<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">Portachiavi</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">Nessun certificato trovato</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">Seleziona certificato</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">"L'applicazione %s ha richiesto un certificato. La selezione di un certificato consente all'applicazione la possibilità di utilizzare questa identità con i server, ora e in futuro."</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">"L'applicazione ha identificato il server richiedente come %s, ma si deve garantire all'applicazione l'accesso al certificato solo se la si ritiene affidabile."</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">È possibile installare certificati da un file PKCS#12 con estensione %1$s o %2$s situati in una memoria esterna.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">"Installa"</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">CONSENTI</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">NEGA</string> 
</resources>
