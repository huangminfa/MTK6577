		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<CheckBox android:text="@string/BandSel_UMTS_BAND_V" android:id="@+id/BandSel_UMTS_BAND_V"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>	
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<CheckBox android:text="@string/BandSel_UMTS_BAND_VI" android:id="@+id/BandSel_UMTS_BAND_VI"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>	
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<CheckBox android:text="@string/BandSel_UMTS_BAND_VII" android:id="@+id/BandSel_UMTS_BAND_VII"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>	
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<CheckBox android:text="@string/BandSel_UMTS_BAND_VIII" android:id="@+id/BandSel_UMTS_BAND_VIII"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>	
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<CheckBox android:text="@string/BandSel_UMTS_BAND_IX" android:id="@+id/BandSel_UMTS_BAND_IX"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>	
		</TableRow>
		
		<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
			<CheckBox android:text="@string/BandSel_UMTS_BAND_X" android:id="@+id/BandSel_UMTS_BAND_X"
			android:layout_width="fill_parent" android:layout_height="wrap_content"></CheckBox>	
		</TableRow>
		
	

	</TableLayout>

<View android:layout_height="1dip" android:layout_width="fill_parent"
			android:background="#FF909090">
</View>

<Button android:id="@+id/BandSel_Btn_Set" android:text="@string/BandSel_SET"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></Button>
	
</LinearLayout>

</ScrollView>
