<?xml version="1.0" encoding="utf-8"?>

<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <include
        latin:keyboardLayout="@xml/kbd_key_styles" />

	<Row latin:keyWidth="10%p">
		<Key latin:keyLabel="\u0308" />
		<Key latin:keyLabel="\u03B5" latin:moreKeys="@string/more_keys_for_e"/>
		<Key latin:keyLabel="\u03C1"  />
		<Key latin:keyLabel="\u03C4"  />
		<Key latin:keyLabel="\u03C5" latin:moreKeys="@string/more_keys_for_y"/>
		<Key latin:keyLabel="\u03B8" />
		<Key latin:keyLabel="\u03B9" latin:moreKeys="@string/more_keys_for_i"/>
		<Key latin:keyLabel="\u03BF" latin:moreKeys="@string/more_keys_for_o"/>
		<Key latin:keyLabel="\u03C0" />
		<Key latin:keyLabel="\u0301"  />
	</Row>
	
	<Row latin:keyWidth="10%p">
		<Key latin:keyLabel="\u03B1" latin:moreKeys="@string/more_keys_for_a"
		     latin:keyXPos="5.0%p" />
		<Key latin:keyLabel="\u03C2"  latin:moreKeys="@string/more_keys_for_x"/>
		<Key latin:keyLabel="\u03B4"  />
		<Key latin:keyLabel="\u03C6"  />
		<Key latin:keyLabel="\u03B3"  />
		<Key latin:keyLabel="\u03B7" latin:moreKeys="@string/more_keys_for_h"/>
		<Key latin:keyLabel="\u03BE"  />
		<Key latin:keyLabel="\u03BA" />
		<Key latin:keyLabel="\u03BB"  latin:keyXPos="85.0%p" />
	</Row>
        
	<Row latin:keyWidth="10%p">
		<Key latin:keyStyle="shiftKeyStyle" latin:keyWidth="15%p" latin:visualInsetsRight="1%p" />
		<Key latin:keyLabel="\u03B6"  />
		<Key latin:keyLabel="\u03C7" />
		<Key latin:keyLabel="\u03C8" />
		<Key latin:keyLabel="\u03C9" latin:moreKeys="@string/more_keys_for_w"/>
		<Key latin:keyLabel="\u03B2" />
		<Key latin:keyLabel="\u03BD" />
		<Key latin:keyLabel="\u03BC" />
		<Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillBoth" latin:visualInsetsLeft="1%p" />
	</Row>
	
    <include
        latin:keyboardLayout="@xml/kbd_qwerty_row4" />
</merge>
