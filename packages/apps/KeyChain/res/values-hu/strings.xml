<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">Kulcsláncolat</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">Nem található tanúsítvány</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">Tanúsítvány választása</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">A(z) %s alkalmazás tanúsítványt kért. Tanúsítvány választásával lehetővé teszi, hogy az alkalmazás az azonosságot szerverekkel használja most és a jövőben is.</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">Az alkalmazás azonosította a kérvényező szervert mint %s, de csak akkor engedélyezze az alkalmazás számára a tanúsítvány elérését, ha az alkalmazást megbízhatónak találja.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">A tanúsítványokat PKCS#12 fájlból telepítheti, amelynek kiterjesztése %1$s vagy %2$s, és a külső tárolón található.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">"Telepítés"</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">ENGEDÉLYEZ</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">MEGTAGAD</string> 
</resources>
