<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">کی چین</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">کوئی سرٹیفکیٹ نہیں ملا</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">سرٹیفیکیٹ منتخب کریں</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">ایپلیکشن %s نے سرٹیفیکیٹ کا مطالبہ کیا ہے۔ سرٹیفیکیٹ منتخب کرنے سے ایپلیکیشن کو ابھی اور مستقبل میں سرورز کے ساتھ یہ شناخت استعمال کرنے کی اہلیت عطا کر دی جائے گی۔</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">ایپلیکیشن نے درخواست کرنے والے سرور کو بطور %s شناخت کیا ہے لیکن آپ صرف اسی صورت میں ایپلیکشن کو سرٹیفیکیٹ تک رسائی کی اجازت فراہم کریں جب آپ ایپلیکشن پر اعتبار کرتے ہوں۔</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">آپ بیرونی اسٹوریج میں واقع %1$s یا %2$s ایکسٹینشن کے ساتھ موجود PKCS#12 فائل سے سرٹیفیکیٹس انسٹال کر سکتے ہیں۔</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">نصب کریں</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">اجازت دیں</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">مسترد</string> 
</resources>
