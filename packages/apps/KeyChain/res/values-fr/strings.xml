<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">Clé</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">Aucun certificat trouvé</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">Sélectionnez un certificat</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">"L'application %s requiert un certificat. La sélection d'un certificat permettra à l'application d'utiliser cette identité avec les serveurs dès maintenant et ultérieurement."</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">"L'application a identifié le serveur effectuant la requête comme %s, mais vous ne devriez autoriser l'accès au certificat que si vous faites confiance à cette application."</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">Vous pouvez installer les certificats depuis un fichier PKCS#12 avec extension %1$s ou %2$s situé sur un stockage externe.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">"Installer"</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">"Autoriser"</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">"Refuser"</string> 
</resources>
