<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">Sleutlketting</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">Geen certificaten gevonden</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">Selecteer certificaat</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">Toepassing %s heeft een certificaat aangevraagd. Door een certificaat te selecteren, krijgt de toepassing de kans om nu en in de toekomst deze identiteit te gebruiken.</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">De toepassing heeft de aanvragende server als %s ge√Ødentificeerd maar u moet de toepassing alleen toegang tot het certificaat geven als u de toepassing vertrouwt.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">U kunt certificaten installeren uit ene PKCS#12-bestand met een extensie %1$s of %2$s in externe opslag.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">"Installeren"</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">TOESTAAN</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">WEIGEREN</string> 
</resources>
