<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">  
    <string name="app_name">Řetězec klíčů</string>  
    <!-- Dialog title when no certificates were found -->  
    <string name="title_no_certs">Nebyl nalezen žádný certifikát</string>  
    <!-- Dialog title when at least one certificate was found -->  
    <string name="title_select_cert">Výběr certifikátu</string>  
    <!-- Used at top of dialog to identify requesting application -->  
    <string name="requesting_application">Aplikace %s požádala o certifikát. Výběrem některého certifikátu zaručíte aplikaci možnost používat tuto identitu se servery nyní a v budoucnosti.</string>  
    <!-- Used at top of dialog to identify requesting server (may be host:port or just host)-->  
    <string name="requesting_server">Aplikace rozpoznala žádající server jako %s, ale musíte aplikaci zaručit přístup k certifikátu pouze, pokud jí důvěřujete.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate (arguments are constants for .pfx and .p12) -->  
    <string name="install_new_cert_message">Můžete nainstalovat certifikáty ze souboru PKCS#12 s příponou %1$s nebo %2$s, které se nacházejí v místním úložišti.</string>  
    <!-- Label of button to send the user to the CertInstaller to install a certificate -->  
    <string name="install_new_cert_button_label">"Instalovat"</string>  
    <!-- Label of button to send the allow the application certificate request -->  
    <string name="allow_button">POVOLIT</string>  
    <!-- Label of button to send the deny the application certificate request -->  
    <string name="deny_button">ODMÍTNOUT</string> 
</resources>
